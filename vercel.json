{
  "version": 2,
  "framework": null,

  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ],

  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/index.html"
    }
  ],

  "routes": [
    { "src": "/Glitched.sw.js", "headers": { "Cache-Control": "no-cache" }, "dest": "/sw.js" }
  ]
}
